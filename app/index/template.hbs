<h1 class="logo align-left">
  <span class="q">Queer</span>
  <span class="t">Tango</span>
  <span class="c">Club</span>
</h1>
{{#file-dropzone name='new-hero'}}
  {{#file-upload name='new-hero'
    onfileadd=(route-action 'changeHero' model)
    accept='image/*'}}
    {{icon 'photo-solid'}} Change Hero
  {{/file-upload}}
{{/file-dropzone}}

{{hero-image hero=model.hero alt=model.name}}
